<?sap.transform simple?>
<tt:transform xmlns:tt="http://www.sap.com/transformation-templates" xmlns:ddic="http://www.sap.com/abapxml/types/dictionary" xmlns:def="http://www.sap.com/abapxml/types/defined">
  <tt:root name="RESPUESTA" type="ddic:ZDTE_TOKEN_RESP"/>
  <tt:template>
    <SII:RESPUESTA xmlns:SII="http://www.sii.cl/XMLSchema">
      <tt:cond s-check="not-initial(RESPUESTA.RESP_BODY.TOKEN)">
        <SII:RESP_BODY>
          <TOKEN tt:value-ref=".RESPUESTA.RESP_BODY.TOKEN"/>
        </SII:RESP_BODY>
      </tt:cond>
      <SII:RESP_HDR>
        <ESTADO tt:value-ref=".RESPUESTA.RESP_HDR.ESTADO"/>
        <GLOSA tt:value-ref=".RESPUESTA.RESP_HDR.GLOSA"/>
      </SII:RESP_HDR>
    </SII:RESPUESTA>
  </tt:template>
</tt:transform>
